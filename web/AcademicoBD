CREATE TABLE veiculo (
    id_veiculo integer NOT NULL,
    nome_veiculo character varying(50) NOT NULL,
    modelo_veiculo character varying(45) NOT NULL,
    fabricante_veiculo character varying(50) NOT NULL,
    placa_veiculo character varying(10) NOT NULL,
    cor_veiculo character varying(30) NOT NULL,
    ano_veiculo character varying(30) NOT NULL,
    chassi_veiculo character varying(25) NOT NULL,
    id_combustivel integer NOT NULL
);


ALTER TABLE public.veiculo OWNER TO postgres;

