CREATE TABLE produto (
    id_produto integer NOT NULL,
    nome_generico character varying(60) NOT NULL,
    origem character varying(35) NOT NULL,
    rotulo_produto character varying(25) NOT NULL,
    painel_seguranca character varying(25) NOT NULL,
    num_risco integer,
    acoes_emerg character varying(80),
    n_lote integer NOT NULL,
    dt_fab date NOT NULL,
    dt_val date NOT NULL,
    composicao character varying(60) NOT NULL,
    desc_produto character varying(60),
    formula_produto character varying,
    odor_produto character varying(25),
    id_embalagem integer NOT NULL,
    id_compatibilidade integer NOT NULL,
    id_num_onu integer,
    id_est_fisico integer,
    id_classe integer,
    id_num_cas integer,
    id_legenda_compatibilidade integer,
    id_armazem integer,
    id_endarmazem integer,
    armazem_id_armazem integer NOT NULL
);


ALTER TABLE public.produto OWNER TO postgres;

--
-- Name: produto_id_produto_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE produto_id_produto_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.produto_id_produto_seq OWNER TO postgres;

--
-- Name: produto_id_produto_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE produto_id_produto_seq OWNED BY produto.id_produto;


--
-- Name: roler; Type: TABLE; Schema: public; Owner: postgres; Tablespace: 
--

CREATE TABLE roler (
    id_roler integer NOT NULL,
    nome_roler character varying(25) NOT NULL
