SET default_with_oids = false;

--
-- Name: armazem; Type: TABLE; Schema: public; Owner: postgres; Tablespace: 
--

CREATE TABLE armazem (
    id_armazem integer NOT NULL,
    tipo_armazem character varying(45) NOT NULL,
    capacidade_armazem character varying(30) NOT NULL,
    certificacao_armazem character varying(45),
    espec_armazem character varying(45),
    estoque_max character varying(30) NOT NULL,
    estoque_min character varying(45) NOT NULL,
    id_status_armazem integer,
    id_endarmazem integer,
    status_armazem_id_status_armazem integer NOT NULL,
    id_local_oper integer,
    local_operacao_id_local_oper integer NOT NULL,
    id_compatibilidade integer NOT NULL
);


ALTER TABLE public.armazem OWNER TO postgres;

--
-- Name: armazem_id_armazem_seq; Type: SEQUENCE; Schema: public; Owner: postgres
--

CREATE SEQUENCE armazem_id_armazem_seq
    START WITH 1
    INCREMENT BY 1
    NO MINVALUE
    NO MAXVALUE
    CACHE 1;


ALTER TABLE public.armazem_id_armazem_seq OWNER TO postgres;

--
-- Name: armazem_id_armazem_seq; Type: SEQUENCE OWNED BY; Schema: public; Owner: postgres
--

ALTER SEQUENCE armazem_id_armazem_seq OWNED BY armazem.id_armazem;


